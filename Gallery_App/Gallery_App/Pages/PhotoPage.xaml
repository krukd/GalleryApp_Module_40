<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Gallery_App.Pages"
             x:Class="Gallery_App.Pages.PhotoPage"
             Title="{x:Static local:PhotoPage.PhotoName}">
	<StackLayout VerticalOptions="Center">
		<Image Source="{x:Static local:PhotoPage.PhotoImage}"/>
		<Label Text="{x:Static local:PhotoPage.PhotoDate}" HorizontalOptions="Center"/>
	</StackLayout>
</ContentPage>
